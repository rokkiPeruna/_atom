"*":
  "animated-page-scroll":
    scrollDuration: 1
    scrollRows: 20
  "atom-fuzzy-grep":
    showFullPath: true
  "bracket-matcher":
    highlightMatchingLineNumber: true
  core:
    telemetryConsent: "limited"
  editor:
    preferredLineLength: 79
    scrollPastEnd: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabType: "soft"
  "exception-reporting":
    userId: "69353531-716e-4c84-ad9a-3f6fa28d88c6"
  "keyboard-scroll":
    animate: false
    animationDuration: 100
    linesToScrollKeydown: 5
    linesToScrollSingle: 5
    throttle: 0
  minimap:
    redrawDelay: 100
  "tree-view": {}
  welcome:
    showOnStartup: false
".json.source":
  editor:
    showIndentGuide: true
".python.regexp.source":
  editor:
    tabLength: 2
".python.source":
  editor:
    showIndentGuide: true
    tabLength: 2
